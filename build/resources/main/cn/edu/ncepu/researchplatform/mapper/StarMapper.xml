<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.edu.ncepu.researchplatform.mapper.StarMapper">

    <resultMap id="BaseResultMap" type="cn.edu.ncepu.researchplatform.entity.Star">
            <id property="peopleId" column="people_id" jdbcType="INTEGER"/>
            <id property="evaluateId" column="evaluate_id" jdbcType="INTEGER"/>
            <result property="flag" column="flag" jdbcType="TINYINT"/>
            <result property="gmtCreate" column="gmt_create" jdbcType="TIMESTAMP"/>
    </resultMap>

    <sql id="Base_Column_List">
        people_id,evaluate_id,flag,
        gmt_create
    </sql>
</mapper>
